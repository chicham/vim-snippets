snippet if # {{#value}} ... {{/value}}
	{{#${1:value}}}
	${0:$TM_SELECTED_TEXT}
	{{/$1}}
snippet ifn # {{^value}} ... {{/value}}
	{{^${1:value}}}
	${0:$TM_SELECTED_TEXT}
	{{/$1}}
snippet ife # {{#value}} ... {{/value}} {{^value}} ... {{/value}}
	{{#${1:value}}}
	${2:$TM_SELECTED_TEXT}
	{{/$1}}
	{{^$1}}
	${3}
	{{/$1}}
